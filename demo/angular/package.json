{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "test:headless": "ng test --code-coverage --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:headless": "cross-env E2E_HEADLESS=true ng e2e",
    "e2e:dev": "ng e2e --webdriver-update=false",
    "e2e:headless:dev": "cross-env E2E_HEADLESS=true ng e2e --webdriver-update=false",
    "precommit": "lint-staged",
    "format": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/common": "~11.2.8",
    "@angular/compiler": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.7",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "~11.2.7",
    "@angular/compiler-cli": "~11.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "4.16.1",
    "@xianshenglu/eslint-config": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "postcss-pixel-to-viewport": "^1.0.2",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "ts-node": "~8.3.0",
    "ts-toolbelt": "^9.6.0",
    "typescript": "~4.1.5"
  },
  "postcss": {
    "plugins": {
      "postcss-pixel-to-viewport": {
        "viewportWidth": 375,
        "viewportUnit": "vw",
        "propertyBlacklist": [
          "letter-spacing"
        ],
        "minPixelValue": 2,
        "enableConvertComment": "on",
        "disableConvertComment": "off",
        "mediaQuery": false
      }
    }
  }
}
