# Copy this file to .env.local and customize for your local environment
# .env.local overrides .env and is not committed to git

# Base URLs for demo projects
# ANGULAR_BASE_URL=http://127.0.0.1:4200/demo/angular/dist/angular/index.html
# REACT_VITE_BASE_URL=http://127.0.0.1:4200/demo/react-vite/dist/index.html
# VUE3_BASE_URL=http://127.0.0.1:4200/demo/vue@3/dist/index.html

# E2E Test Configuration
# E2E_HEADLESS=false
# OPEN_REPORT=true
# CI=false

# Proxy Configuration (optional)
# PROXY_SERVER=http://proxy.example.com:8080
# PROXY_USERNAME=your-username
# PROXY_PASSWORD=your-password

